{
  "name": "deploy",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "lint": "eslint src --ext ts --report-unused-disable-directives --max-warnings 0",
    "typecheck": "tsc --noEmit",
    "prebuild": "rm -rf dist",
    "build": "esbuild src/lambdas/**/index.ts --bundle --platform=node --sourcemap --outdir=dist",
    "deploy": "cdktf deploy --auto-approve '*'"
  },
  "dependencies": {
    "@cdktf/provider-aws": "14.0.4",
    "@tsukiy0/cdktf": "*",
    "@vendia/serverless-express": "4.10.3",
    "cdktf": "0.16.1",
    "constructs": "10.2.29",
    "express": "4.18.2"
  },
  "devDependencies": {
    "@tsukiy0/core": "*",
    "@tsukiy0/core-aws": "*",
    "@tsukiy0/eslint-config-custom": "*",
    "@types/aws-lambda": "8.10.115",
    "@types/express": "4.17.17",
    "cdktf-cli": "0.16.1",
    "esbuild": "0.17.19",
    "ts-node": "10.9.1",
    "tsconfig": "*",
    "typescript": "5.0.4"
  }
}
